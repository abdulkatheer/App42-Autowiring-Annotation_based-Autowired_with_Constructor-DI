<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id="acc1" class="org.katheer.bean.Account">
        <property name="accNo" value="B101"/>
        <property name="bank" value="IOB"/>
        <property name="branch" value="Dindigul"/>
        <property name="accName" value="Abdul Katheer"/>
        <property name="balance" value="670000"/>
    </bean>

    <bean id="acc2" class="org.katheer.bean.Account">
        <property name="accNo" value="B102"/>
        <property name="bank" value="SBI"/>
        <property name="branch" value="Trichy"/>
        <property name="accName" value="Dhanalakshmi"/>
        <property name="balance" value="60000"/>
    </bean>

    <bean id="emp1" class="org.katheer.bean.Employee">
        <constructor-arg value="Abdul Katheer" index="0"/>
        <constructor-arg value="E101" index="1"/>
    </bean>
</beans>